@article{Chrysochoidis2004,
abstract = {Experimental and analytical studies are presented investigating the$\backslash$neffect of delamination cracks on the low-frequency dynamic response of$\backslash$ncomposite beam specimens with surface attached piezoelectric actuators$\backslash$nand sensors. Frequency response functions of laminated beams with$\backslash$npiezoceramic actuators and piezoelectric sensors having single$\backslash$ndelamination cracks of various sizes are measured. Comparisons with$\backslash$nmechanical actuation-accelerometer sensor configurations illustrate the$\backslash$nsufficiency and advantages of piezoelectric actuator-sensor pairs in$\backslash$nself-monitoring the effects of delamination on the dynamic response.$\backslash$nModal frequencies and damping values of the beams are also measured and$\backslash$ntheir dependence on delamination size is studied. Comparisons with$\backslash$nanalytically predicted results are shown. Finally, the sensitivity of$\backslash$ndamage indices based on changes in modal frequency, modal damping, and$\backslash$nmodal peak density to the crack size is quantified.},
